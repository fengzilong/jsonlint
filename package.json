{
  "name": "@biu/jsonlint",
  "version": "1.1.1",
  "description": "lint your json, report all errors at once",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:jest --",
    "lint": "eslint ./lib --cache --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "json",
    "lint",
    "comment",
    "syntax",
    "error"
  ],
  "author": "fengzilong",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "emphasize": "^4.2.0",
    "vscode-json-languageservice": "^4.1.6"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-mo": "^0.5.3",
    "husky": ">=6",
    "jest": "^27.0.6",
    "lint-staged": ">=10"
  },
  "repository": "github:fengzilong/jsonlint",
  "homepage": "https://github.com/fengzilong/jsonlint#readme",
  "bugs": {
    "url": "https://github.com/fengzilong/jsonlint/issues"
  },
  "lint-staged": {
    "lib/*.js": "eslint --cache --fix"
  },
  "engines": {
    "node": ">=10"
  }
}
